<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_focused="true" android:state_enabled="false">
		<shape android:shape="0">
			<corners android:radius="3dp" />
			<solid android:color="84878c" />
		</shape>
	</item>
	<item android:state_focused="false" android:state_enabled="false">
		<shape android:shape="0">
			<corners android:radius="3dp" />
			<solid android:color="bdc1c9" />
		</shape>
	</item>
	<item android:state_focused="true">
		<shape android:shape="0">
			<corners android:radius="3dp" />
			<solid android:color="fffffe" />
			<stroke android:width="1dp" android:color="dedee6" />
		</shape>
	</item>
	<item android:state_pressed="true">
		<shape android:shape="0">
			<corners android:radius="3dp" />
			<solid android:color="2f477a" />
		</shape>
	</item>
	<item>
		<shape android:shape="0">
			<corners android:radius="3dp" />
			<solid android:color="4267b2" />
		</shape>
	</item>
</selector>
